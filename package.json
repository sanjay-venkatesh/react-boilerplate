{
  "name": "react-boilerplate-master",
  "version": "1.0.0",
  "description": "React client boilerplate",
  "author": "Sanjay Venkatesh",
  "scripts": {
    "start": "webpack-dev-server --port 8888 --inline --hot --host 0.0.0.0 --config webpack/webpack.dev.js",
    "proxy": "webpack-dev-server --hot --inline --config webpack/webpack.proxy.js",
    "build": "webpack --config webpack/webpack.prod.js",
    "analyzer": "webpack --config webpack/webpack.analyzer.js",
    "storybook": "start-storybook -p 9000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sanjay-venkatesh/react-boilerplate"
  },
  "dependencies": {
    "@babel/core": "7.4.0",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.4.2",
    "@babel/preset-react": "7.0.0",
    "antd": "3.13.0",
    "axios": "0.18.0",
    "babel-loader": "8.0.5",
    "classnames": "2.2.5",
    "clean-webpack-plugin": "1.0.1",
    "core-js": "2.6.1",
    "css-loader": "1.0.0",
    "extract-css-chunks-webpack-plugin": "3.3.2",
    "file-loader": "2.0.0",
    "i18next": "10.2.1",
    "i18next-xhr-backend": "1.5.1",
    "jquery": "2.2.3",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "moment": "2.22.0",
    "normalizr": "3.3.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "prop-types": "15.6.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-i18next": "7.10.1",
    "react-redux": "4.4.9",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "redux": "3.5.2",
    "redux-devtools-extension": "2.13.7",
    "redux-thunk": "2.1.0",
    "reselect": "4.0.0",
    "style-loader": "0.23.0",
    "uglifyjs-webpack-plugin": "1.3.0",
    "url-loader": "1.1.1",
    "webpack": "4.17.2",
    "webpack-cli": "3.1.2",
    "webpack-manifest-plugin": "2.0.4",
    "webpack-merge": "4.1.4"
  },
  "devDependencies": {
    "@storybook/react": "5.1.11",
    "babel-eslint": "8.2.3",
    "eslint": "5.0.0",
    "eslint-plugin-css-modules": "2.10.0",
    "eslint-plugin-react": "7.11.1",
    "prettier": "1.14.2",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-dev-server": "3.1.10"
  }
}
